@model System.Web.Mvc.PagedList<App_Dominio.Repositories.TransacaoRepository>
@{
    Layout = null;
    Model.action = "../Log/_ListTransacaoModal";
}

<div class="row">
    <div class="col-md-11" style="margin: 0px">
        @Html.Partial("_Filter", Model)
        <div id="div-list" style="padding-top: 0px">
            @Html.Partial("../Log/_ListTransacaoModal", Model)
        </div>
    </div>
</div>

<script type="text/javascript">
    function Move(id, descricao) {
        $("#transacaoId").val(id);
        $("#transacao1Id").val(id);
        $("#nome_transacao").val(descricao);
        $("#nome_transacao1").val(descricao);
        $("#btnModal").click();
    }
</script>

